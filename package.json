{
  "name": "qiaojun",
  "version": "1.0.0",
  "description": "npm chuxin-cs Repository, Linkï¼šhttps://www.npmjs.com/settings/qiaojun/packages",
  "homepage": "https://github.com/chuxin-cs/qiaojun",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chuxin-cs/qiaojun.git"
  },
  "keywords": [
    "qiaojun",
    "npm"
  ],
  "author": "chuxin",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r --if-present run build",
    "build:packages": "pnpm -r --if-present --filter \"./packages/**\" run build",
    "build:utils": "pnpm -r --filter @qiaojun/utils run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build:packages && changeset publish",
    "release:dry": "pnpm build:packages && changeset publish --no-git-tag --dry",
    "changeset:status": "changeset status"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.0",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.26"
  }
}
